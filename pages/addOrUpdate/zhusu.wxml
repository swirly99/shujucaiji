  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">

        <view class="tr_view" style="height:304rpx">
          <view class="td_left" style="width:30%;line-height:304rpx"><text space="ensp">住宿分类： </text></view>
          <view class="td_right" style="width:70%">
              <radio-group class="radio-group" bindchange="input_value" data-id="zsfl">
                <view class="box" wx:for-items="{{zsfl_item}}">
                  <radio value="{{item.name}}" checked="{{item.checked}}"><text space="ensp">{{item.name}}    </text></radio>
                </view>
              </radio-group>
          </view>
        </view>

        <view class="tr_view" style="height:304rpx">
          <view class="td_left" style="width:30%;line-height:304rpx"><text space="ensp">简介： </text></view>
          <view class="td_right" style="width:70%">
            <textarea maxlength="-1" placeholder="请输入简介" bindinput="input_value" data-id="description">{{description}}</textarea>
          </view>
        </view>
        
      </view>
      <button bindtap="up_view" style="float:left;width:{{b_width}}px" class="left_button">上一页</button>
      <button bindtap="submit" style="float:right;background:#F7B267;width:{{b_width}}px" class="right_button">提交</button>
