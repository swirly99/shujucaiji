        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">服务电话： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入服务电话" bindinput="input_value" data-id="fwdh" value="{{fwdh}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left font-small"><text space="ensp">指定团餐点： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入指定团餐点" bindinput="input_value" data-id="zdtcd" value="{{zdtcd}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">交通路线： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入交通路线" bindinput="input_value" data-id="jtlx" value="{{jtlx}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">开业时间： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入开业时间" bindinput="input_value" data-id="kysj" value="{{kysj}}"/>
            <picker mode="date" value="{{kysj}}" bindchange="time_change" data-id="kysj" >

            </picker>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">餐位数： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入餐位数" bindinput="input_value" data-id="bjscws" value="{{bjscws}}"/>
