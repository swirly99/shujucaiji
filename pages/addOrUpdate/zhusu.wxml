<swiper current="{{view_index}}" bindchange="change_swiper">

  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view">
          <view class="td_left"><text space="ensp">住宿名称： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入住宿名称" bindinput="input_value" data-id="name" value="{{name}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">用户名： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入住宿名称" bindinput="input_value" data-id="userName" value="{{userName}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">营业状态： </text></view>
          <view class="td_right" style="padding-top:25rpx;height:65rpx">
            <radio-group class="radio-group" bindchange="input_value" data-id="yyzt">
              <radio wx:for-items="{{yyzt_item}}" value="{{item.name}}" checked="{{item.checked}}"><text space="ensp">{{item.name}}    </text></radio>
            </radio-group>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left font-small"><text space="ensp">组织机构代码： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入组织机构代码" bindinput="input_value" data-id="zzjg" value="{{zzjg}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">行政区划： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入行政区划" bindinput="input_value" data-id="xzqh" value="{{xzqh}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">详细地址： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入住宿地址" bindinput="input_value" data-id="xxdz" value="{{xxdz}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">预订和投诉电话： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入预订和投诉电话" bindinput="input_value" data-id="phone" value="{{phone}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">星级： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入星级" bindinput="input_value" data-id="level" value="{{level}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">联系方式： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入联系方式" bindinput="input_value" data-id="email" value="{{email}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">所属菜系： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入所属菜系" bindinput="input_value" data-id="sscx" value="{{sscx}}"/>
          </view>
        </view>
      </view>
      <button bindtap="bottom_view" style="width:{{b_width}}px">下一页</button>
    </view>
  </swiper-item>

  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view">
          <view class="td_left"><text space="ensp">房价： </text></view>
          <view class="td_right">
            <input type="digit" placeholder="请输入房价" bindinput="input_value" data-id="price" value="{{price}}"/>
          </view>
        </view>
        <view class="tr_view">
          <view class="td_left"><text space="ensp">营业执照： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入营业执照" bindinput="input_value" data-id="yyzzbh" value="{{yyzzbh}}"/>
