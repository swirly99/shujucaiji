    </view>
  </swiper-item>

  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view">
          <view class="td_left"><text space="ensp">酒店政策： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入酒店政策" bindinput="input_value" data-id="jdzc" value="{{jdzc}}"/>
          </view>
        </view>

        <view class="tr_view">
          <view class="td_left font-small"><text space="ensp">交通线路： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入交通线路" bindinput="input_value" data-id="jtxl" value="{{jtxl}}"/>
          </view>
        </view>

        <view class="tr_view">
          <view class="td_left"><text space="ensp">标识图片： </text></view>
          <view class="td_right"></view>
        </view>

        <view class="tr_view">
          <view class="td_left"><text space="ensp">住宿图片： </text></view>
          <view class="td_right"></view>
        </view>

        <view class="tr_view">
          <view class="td_left"><text space="ensp">标志图： </text></view>
          <view class="td_right"></view>
        </view>

        <view class="tr_view">
          <view class="td_left"><text space="ensp">房间数量： </text></view>
          <view class="td_right">
             <input type="number" placeholder="请输入房间数量" bindinput="input_value" data-id="fjsl" value="{{fjsl}}"/>
          </view>
        </view>

        <view class="tr_view">
          <view class="td_left"><text space="ensp">信用卡： </text></view>
          <view class="td_right">
             <input type="text" placeholder="请输入信用卡" bindinput="input_value" data-id="xyk" value="{{xyk}}"/>
          </view>
        </view>

        <view class="tr_view">
          <view class="td_left"><text space="ensp">房间类型： </text></view>
          <view class="td_right">
             <input type="text" placeholder="请输入房间类型" bindinput="input_value" data-id="fjlx" value="{{fjlx}}"/>
          </view>
        </view>

