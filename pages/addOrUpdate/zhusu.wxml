            <input type="number" placeholder="请输入停车位数量" bindinput="input_value" data-id="tcwsl" value="{{tcwsl}}"/>
          </view>
        </view>
      </view>
      <button bindtap="up_view" style="float:left;width:{{b_width}}px" class="left_button">上一页</button>
      <button bindtap="bottom_view" style="float:right;width:{{b_width}}px" class="right_button">下一页</button>
    </view>
  </swiper-item>

  <swiper-item>
