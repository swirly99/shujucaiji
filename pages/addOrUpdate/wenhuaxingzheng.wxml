<!--pages/addOrUpdate/wenhuaxingzheng.wxml-->
<!-- <text>pages/addOrUpdate/wenhuaxingzheng.wxml</text> -->
<swiper current="{{view_index}}" bindchange="change_swiper">
  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">从业人员数：</text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入从业人员数" bindinput="input_value" data-id="Practitioners" value="{{Practitioners}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">事业编制人数：</text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入事业编制人数" bindinput="input_value" data-id="Career" value="{{Career}}"/>
          </view>
        </view>
      </view>
      <button bindtap="submit" style="background:#F7B267;width:{{b_width}}rpx;font-size:36rpx">提交</button>
    </view>
  </swiper-item>
</swiper>

