<!--pages/addOrUpdate/tesecanyin.wxml-->
<!-- <text>pages/addOrUpdate/tesecanyin.wxml</text> -->
<swiper current="{{view_index}}" bindchange="change_swiper">
  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">特色餐饮点： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入特色餐饮点" bindinput="input_value" data-id="SpeRestaurant" value="{{SpeRestaurant}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">所属菜系： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入所属菜系" bindinput="input_value" data-id="cuisine" value="{{cuisine}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">指定团餐点： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入指定团餐点" bindinput="input_value" data-id="GroupMeal" value="{{GroupMeal}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">特色餐饮 </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入特色餐饮" bindinput="input_value" data-id="FoodSpecial" value="{{FoodSpecial}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">包间数： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入包间数" bindinput="input_value" data-id="PrivateRooms" value="{{PrivateRooms}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">餐位数： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入餐位数" bindinput="input_value" data-id="seat" value="{{seat}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">营业面积： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入营业面积" bindinput="input_value" data-id="area" value="{{area}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">WIFI 情况： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入WIFI 情况" bindinput="input_value" data-id="isWifi" value="{{isWifi}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">信用卡类型： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入信用卡类型" bindinput="input_value" data-id="CardType" value="{{CardType}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">餐厅等级： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入餐厅等级" bindinput="input_value" data-id="grade" value="{{grade}}"/>
          </view>
        </view>
      </view>
      <button bindtap="bottom_view" style="width:{{b_width}}rpx">下一页</button>
    </view>
  </swiper-item>
  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">就餐方式： </text></view>
          <view class="td_right">
            <input type="text" placeholder="请输入就餐方式" bindinput="input_value" data-id="DiningMode" value="{{DiningMode}}"/>
          </view>
        </view>
      </view>
      <button bindtap="up_view" style="float:left;width:{{b_width}}rpx" class="left_button">上一页</button>
      <button bindtap="submit" style="float:right;background:#F7B267;width:{{b_width}}rpx;font-size:36rpx" class="right_button">提交</button>
    </view>
  </swiper-item>
</swiper>

