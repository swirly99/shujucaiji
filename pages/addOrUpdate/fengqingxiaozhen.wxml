<!--pages/addOrUpdate/fengqingxiaozhen.wxml-->
<!-- <text>pages/addOrUpdate/fengqingxiaozhen.wxml</text> -->
<swiper current="{{view_index}}" bindchange="change_swiper">
  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view" style="height:200rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">风情小镇创建阶段： </text>
            <textarea  placeholder="请输入授予风情小镇创建阶段" bindinput="input_value" data-id="CreateLevel" value="{{CreateLevel}}" style="height:50%"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">床位数： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入床位数" bindinput="input_value" data-id="BedNum" value="{{BedNum}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left"><text space="ensp">餐位数： </text></view>
          <view class="td_right">
            <input type="number" placeholder="请输入餐位数" bindinput="input_value" data-id="MealSum" value="{{MealSum}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left" style="width:40%"><text space="ensp">申报区域范围 ： </text></view>
          <view class="td_right" style="width:60%">
            <input type="text" placeholder="请输入申报区域范围" bindinput="input_value" data-id="area" value="{{area}}"/>
          </view>
        </view>
        <view class="tr_view" >
          <view class="td_left" style="width:45%"><text space="ensp">核心区四至边界 ： </text></view>
          <view class="td_right" style="width:55%">
            <input type="text" placeholder="请输入核心区四至边界" bindinput="input_value" data-id="CoreArea" value="{{CoreArea}}"/>
          </view>
        </view>
        <view class="tr_view" style="height:250rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">自然风景： </text>
            <textarea  placeholder="请输入自然风景" bindinput="input_value" data-id="NaturalScenery" value="{{NaturalScenery}}" style="height:60%"/>
          </view>
        </view>
      </view>
      <button bindtap="bottom_view">下一页</button>
    </view>
  </swiper-item>
  <swiper-item>
    <view class="bg_view">
      <view style="width:100%;height:1020rpx">
        <view class="tr_view" style="height:180rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">建筑风貌： </text>
            <textarea  placeholder="请输入建筑风貌" bindinput="input_value" data-id="BuildStyle" value="{{BuildStyle}}"/>
          </view>
        </view>
        <view class="tr_view" style="height:180rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">节会风俗： </text>
            <textarea  placeholder="请输入节会风俗" bindinput="input_value" data-id="custom" value="{{custom}}"/>
          </view>
        </view>
        <view class="tr_view" style="height:180rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">特产风物： </text>
            <textarea  placeholder="请输入特产风物" bindinput="input_value" data-id="SpecialtyProducts" value="{{SpecialtyProducts}}"/>
          </view>
        </view>
        <view class="tr_view" style="height:180rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">餐饮风味： </text>
            <textarea  placeholder="请输入餐饮风味" bindinput="input_value" data-id="CateringFlavor" value="{{CateringFlavor}}"/>
          </view>
        </view>
        <view class="tr_view" style="height:180rpx" >
          <view class="td_left" style="width:100%">
            <text space="ensp">人物风采： </text>
            <textarea  placeholder="请输入人物风采" bindinput="input_value" data-id="PDem" value="{{PDem}}"/>
          </view>
        </view>
      </view>
      <button bindtap="up_view" style="float:left;width:{{b_width}}rpx" class="left_button">上一页</button>
      <button bindtap="submit" style="float:right;background:#F7B267;width:{{b_width}}rpx;font-size:36rpx" class="right_button">提交</button>
    </view>
  </swiper-item>
</swiper>